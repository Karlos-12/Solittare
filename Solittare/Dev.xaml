<Window x:Class="Solittare.Dev"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Solittare"
        mc:Ignorable="d"
        Title="Dev" Height="450" Width="800" Foreground="{x:Null}" Background="#FF2F2F2F">
    <Grid>
        <Button x:Name="cl" Content="Clear all" HorizontalAlignment="Left" Height="60" Margin="45,65,0,0" VerticalAlignment="Top" Width="102" Click="cl_Click"/>
        <Button x:Name="cl_Copy" Content="Add win" HorizontalAlignment="Left" Height="60" Margin="243,108,0,0" VerticalAlignment="Top" Width="102" Click="addwin"/>
        <Button x:Name="upl" Content="Upload image" HorizontalAlignment="Left" Height="60" Margin="372,0,0,0" VerticalAlignment="Center" Width="102" Click="upload"/>
        <Button x:Name="ld" Content="Loaf" HorizontalAlignment="Left" Height="60" Margin="282,274,0,0" VerticalAlignment="Top" Width="102" Click="ggg"/>
        <TextBox x:Name="t" HorizontalAlignment="Left" Height="40" Margin="400,294,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="74"/>

    </Grid>
</Window>
